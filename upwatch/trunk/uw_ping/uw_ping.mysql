--
-- Table structure for table 'pr_ping_def'
--

CREATE TABLE pr_ping_def (
  id int(8) NOT NULL auto_increment,
  server int(8) NOT NULL default '0',
  address varchar(80) NOT NULL default '',
  freq int(4) NOT NULL default '1',
  count int(2) unsigned NOT NULL default '5',
  yellowmiss int(2) unsigned NOT NULL default '1',
  redmiss int(2) unsigned NOT NULL default '3',
  stattime int(10) unsigned NOT NULL default '0',
  expires int(10) unsigned NOT NULL default '0',
  color smallint(5) unsigned NOT NULL default '0',
  lasthist int(10) unsigned NOT NULL default '0',
  message text NOT NULL,
  PRIMARY KEY  (id)
) TYPE=MyISAM;

