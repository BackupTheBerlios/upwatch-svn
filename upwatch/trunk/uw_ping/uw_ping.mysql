--
-- Table structure for table 'pr_ping_5year'
--

CREATE TABLE pr_ping_5year (
  id int(10) unsigned NOT NULL auto_increment,
  probe int(10) unsigned NOT NULL default '0',
  hist int(10) unsigned NOT NULL default '0',
  slot int(4) NOT NULL default '0',
  stattime int(10) unsigned NOT NULL default '0',
  color smallint(5) unsigned NOT NULL default '0',
  value float NOT NULL default '0',
  lowest float NOT NULL default '0',
  highest float NOT NULL default '0',
  PRIMARY KEY  (id),
  UNIQUE KEY probtime (probe,stattime),
  KEY probe (probe),
  KEY hist (hist),
  KEY slot (slot),
  KEY stattime (stattime)
) TYPE=MyISAM;

