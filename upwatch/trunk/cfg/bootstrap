#! /bin/sh
set -x
# libstatgrab
cd libstatgrab
if [ ! -f libstatgrab-configure.gnu ]
then
  cp ../patches/libstatgrab-configure.gnu .
#  aclocal
#  libtoolize
#  automake --gnu --add-missing
#  autoconf
fi
cd ..

aclocal \
&& autoheader \
&& libtoolize \
&& automake --gnu --add-missing \
&& autoconf
